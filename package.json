{
  "name": "ember-resolver",
  "version": "7.0.0",
  "description": "The default modules based resolver for Ember CLI.",
  "keywords": [
    "ember-addon"
  ],
  "homepage": "https://github.com/ember-cli/ember-resolver#readme",
  "bugs": {
    "url": "https://github.com/ember-cli/ember-resolver/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ember-cli/ember-resolver.git"
  },
  "license": "MIT",
  "author": "Robert Jackson <me@rwjblue.com>",
  "main": "index.js",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "changelog": "lerna-changelog",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each"
  },
  "dependencies": {
    "babel-plugin-debug-macros": "^0.3.3",
    "broccoli-funnel": "^3.0.2",
    "broccoli-merge-trees": "^4.2.0",
    "ember-cli-babel": "^7.18.0",
    "ember-cli-version-checker": "^5.0.2",
    "resolve": "^1.15.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "babel-eslint": "^10.0.3",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.14.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "^4.2.2",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-qunit": "^4.1.1",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-load-initializers": "^2.1.1",
    "ember-qunit-assert-helpers": "^0.2.2",
    "ember-source": "~3.16.5",
    "ember-source-channel-url": "^2.0.1",
    "ember-try": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-plugin-ember": "^7.8.1",
    "eslint-plugin-node": "^10.0.0",
    "lerna-changelog": "^0.8.3",
    "loader.js": "^4.7.0",
    "qunit-dom": "^0.9.0",
    "release-it": "^13.5.2",
    "release-it-lerna-changelog": "^1.0.3"
  },
  "engines": {
    "node": ">= 10.*"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md"
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    }
  }
}
